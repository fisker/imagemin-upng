{
  "name": "imagemin-upng",
  "version": "2.0.2",
  "description": "upng imagemin plugin",
  "keywords": [
    "apng",
    "compress",
    "image",
    "imageminplugin",
    "img",
    "minify",
    "optimize",
    "png",
    "upng",
    "upng-js"
  ],
  "homepage": "https://github.com/fisker/imagemin-upng#readme",
  "bugs": {
    "url": "https://github.com/fisker/imagemin-upng/issues"
  },
  "repository": "fisker/imagemin-upng",
  "funding": "https://github.com/fisker/imagemin-upng?sponsor=1",
  "license": "MIT",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:js": "rollup --config",
    "clean": "run-p clean:*",
    "clean:dist": "del-cli dist",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:markdown": "yarn lint:markdown --fix",
    "format:package-json": "sort-package-json \"package.json\" \"packages/*/package.json\"",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:package-json": "yarn run format:package-json --check",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s clean build lint test format dist",
    "test": "ava",
    "test-coverage": "nyc yarn test"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "babel": true,
    "require": [
      "esm"
    ],
    "verbose": true
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "dependencies": {
    "is-png": "2.0.0",
    "lib-upng": "1.0.2"
  },
  "devDependencies": {
    "@ava/babel": "1.0.1",
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-object-rest-spread": "7.9.0",
    "@babel/preset-env": "7.9.0",
    "@commitlint/cli": "8.3.5",
    "@fisker/commitlint-config": "1.1.7",
    "@fisker/eslint-config": "2.0.0",
    "@fisker/eslint-config-ava": "1.2.9",
    "@fisker/husky-config": "2.0.0",
    "@fisker/lint-staged-config": "2.0.0",
    "@fisker/prettier-config": "2.0.0",
    "ava": "3.5.1",
    "babel-plugin-transform-async-to-promises": "0.8.15",
    "cz-conventional-changelog": "3.1.0",
    "del-cli": "3.0.0",
    "eslint": "6.8.0",
    "esm": "3.2.25",
    "husky": "4.2.3",
    "lint-staged": "10.0.8",
    "markdownlint-cli": "0.22.0",
    "npm-run-all": "4.1.5",
    "nyc": "15.0.0",
    "prettier": "2.0.1",
    "rollup": "2.1.0",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-prettier": "0.6.0",
    "sort-package-json": "1.40.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
