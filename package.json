{
  "name": "imagemin-upng",
  "version": "1.3.0",
  "description": "upng imagemin plugin",
  "license": "MIT",
  "repository": "fisker/imagemin-upng",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "keywords": [
    "compress",
    "image",
    "imageminplugin",
    "img",
    "minify",
    "optimize",
    "png",
    "apng",
    "upng",
    "upng-js"
  ],
  "dependencies": {
    "is-png": "2.0.0",
    "upng-js": "2.1.0"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:js": "rollup --config",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s build lint test format dist",
    "test": "ava",
    "test-coverage": "nyc ava"
  },
  "devDependencies": {
    "@babel/cli": "7.6.2",
    "@babel/core": "7.6.2",
    "@babel/preset-env": "7.6.2",
    "@commitlint/cli": "8.2.0",
    "@fisker/commitlint-config": "1.1.3",
    "@fisker/eslint-config": "1.3.2",
    "@fisker/eslint-config-ava": "1.2.3",
    "@fisker/husky-config": "1.1.3",
    "@fisker/lint-staged-config": "1.0.5",
    "@fisker/prettier-config": "1.0.21",
    "ava": "2.4.0",
    "babel-plugin-transform-async-to-promises": "0.8.14",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "6.4.0",
    "esm": "3.2.25",
    "husky": "3.0.5",
    "lint-staged": "9.3.0",
    "markdownlint-cli": "0.18.0",
    "npm-run-all": "4.1.5",
    "nyc": "14.1.1",
    "prettier": "1.18.2",
    "rollup": "1.21.4",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-filesize": "6.2.0",
    "rollup-plugin-prettier": "0.6.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "ava": {
    "require": "esm",
    "verbose": true
  },
  "nyc": {
    "require": "esm",
    "reporter": [
      "lcov",
      "text"
    ],
    "include": [
      "src/**"
    ]
  },
  "sideEffects": false
}
